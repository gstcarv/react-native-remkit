{
    "name": "@remkit/cli",
    "version": "1.0.0",
    "description": "Interactive CLI for RemKit",
    "main": "dist/index.js",
    "bin": {
        "remkit": "./dist/index.js"
    },
    "scripts": {
        "build": "tsc && cp src/commands/init/remkit.config.json dist/commands/init/remkit.config.json",
        "dev": "tsc --watch",
        "start": "tsx src/index.ts",
        "start:dev": "tsx watch src/index.ts",
        "check-types": "tsc --noEmit"
    },
    "keywords": [
        "cli",
        "remkit",
        "react-native"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@babel/plugin-transform-runtime": "^7.28.5",
        "@babel/runtime": "^7.28.4",
        "@react-native/babel-preset": "^0.82.1",
        "@babel/plugin-transform-async-generator-functions": "^7.20.0",
        "@babel/core": "^7.25.0",
        "@babel/preset-env": "^7.25.0",
        "@babel/preset-react": "^7.25.0",
        "@babel/preset-typescript": "^7.25.0",
        "babel-loader": "^9.2.0",
        "commander": "^12.1.0",
        "webpack": "^5.97.0"
    },
    "devDependencies": {
        "@types/node": "^22.10.5",
        "@types/webpack": "^5.28.5",
        "tsx": "^4.19.2",
        "typescript": "5.9.2"
    }
}